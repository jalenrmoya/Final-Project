		{
			"character": "character-1681932524.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "..."
		},
		{
			"character": "character-1681933194.json",
			"event_id": "dialogic_001",
			"portrait": "Amelia_Angry",
			"text": "Goodbye, Trent. I never want to see you again."
		},
		{
			"character": "character-1681933200.json",
			"event_id": "dialogic_001",
			"portrait": "Kate_Angry",
