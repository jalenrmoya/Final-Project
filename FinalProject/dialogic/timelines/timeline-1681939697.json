		},
		{
			"character": "character-1681932524.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "(She smiled at me, then left the room.)"
		},
		{
			"character": "character-1681932524.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "I’m gonna spend the rest of my life trying to forget…"
		},
		{
			"animation": "slide_in_right.gd",
			"animation_length": 0,
			"change_mirror_portrait": false,
